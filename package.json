{
  "name": "react-fuzzy-filter",
  "version": "3.2.0",
  "description": "Render a subset of a collection with fuzzy searching",
  "main": "dist/react-fuzzy-filter.umd.min.js",
  "module": "dist/react-fuzzy-filter.es-modules.js",
  "scripts": {
    "build": "yarn run clean && $(yarn bin)/rollup -c && yarn run minify",
    "minify": "yarn run minify-browser && yarn run minify-umd",
    "minify-browser": "$(yarn bin)/uglifyjs dist/react-fuzzy-filter.browser.js -cm > dist/react-fuzzy-filter.browser.min.js",
    "minify-umd": "$(yarn bin)/uglifyjs dist/react-fuzzy-filter.umd.js -cm > dist/react-fuzzy-filter.umd.min.js",
    "lint:src": "$(yarn bin)/eslint ./src",
    "lint:test": "$(yarn bin)/eslint ./test",
    "lint": "yarn run lint:src && yarn run lint:test",
    "test": "$(yarn bin)/jest",
    "clean": "rm -rf dist",
    "precommit": "lint-staged",
    "prepublish": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdlehman/react-fuzzy-filter.git"
  },
  "bugs": {
    "url": "https://github.com/jdlehman/react-fuzzy-filter/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "filter",
    "fuzzy",
    "javascript"
  ],
  "files": [
    "dist"
  ],
  "author": "Jonathan Lehman <jonathan.lehman91@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^22.0.3",
    "husky": "^0.14.3",
    "jest": "^22.0.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "uglify-js": "^3.1.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "debounce": "^1.0.2",
    "fuse.js": "^3.1.0",
    "prop-types": "^15.6.0",
    "rxjs": "^5.4.3"
  }
}
